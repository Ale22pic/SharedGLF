FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 6.478e-5; 

boundaryField
{

    Freestream
    {
	type            fixedValue;
        value           uniform 6.478e-5;
    }
    Inlet
    {
	type            fixedValue;
        value           uniform 6.478e-5;
    }
    Outlet
    {
	type            fixedValue;
        value           uniform 6.478e-5;
    }
    Sym
    {
        type            symmetry;
    }
    Wing
    {
	type            fixedValue;
        value           uniform 0;
    }

}


