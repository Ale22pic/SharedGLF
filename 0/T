FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

Tinlet          304.2;

dimensions      [0 0 0 1 0 0 0];

internalField   uniform $Tinlet;

boundaryField
{
    Freestream
    {
        type            inletOutlet;
        inletValue      uniform $Tinlet;
        value           $inletValue;
    }
    Inlet
    {
	type            inletOutlet;
        inletValue      uniform $Tinlet;
        value           $inletValue;
    }
    Outlet
    {
        type            inletOutlet;
        inletValue      uniform $Tinlet;
        value           $inletValue;
    }
    Sym
    {
        type            symmetry;
    }
    Wing
    {
        type            zeroGradient;
    }

}

